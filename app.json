{
  "expo": {
    "name": "Antar",
    "slug": "antar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/profileLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bbinnovations.antar",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "softwareKeyboardLayoutMode": "pan",
        "foregroundImage": "./assets/images/profileLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bbinnovations.antar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0646eda2-ed4e-4aca-bf7b-f7463bca4fd9"
      }
    },
    "owner": "bbinnovations"
  }
}
